	// box11
	.box11
		float left
		width 100%
		height 450px
		background #f1f1f1
		.col-4
			float left
			border-right: 1px solid grey
			a
				img
					margin-top 10%
			p
				margin-top 10%
				margin-left 10%
				color #c5bfbc
			h5
				margin-left 10%
				color #c5bfbc
			.anh
				width 100%
				height 100px
				margin-left 10%
				a.pay
					float left
					padding-right 1%
					img
						width 60px
		.col-8
			float left
			.o1
				width 100%
				height 40%
				margin-left 5%
				h4
					font-weight 300
					padding-top 5%
				form
					.form-group
						float left
						width 60%
						input
							height 50px
					.btn
						float left
						margin-left 0.5%
						width 150px
						height 50px
						background black
						&:hover
							background red
				hr
					width 80%
					float left
			.o2
				width 100%
				height 60%
				.col-md-4
					float left
					h4
						padding-left 13%
						color #c5bfbc
						font-weight 300
						padding-top 5%
					ul
						li
							list-style-type none
							padding-top 3%
							a
								text-decoration none
								color text-color
								&:hover
									padding-left 5px
									color red
									transition .9s ease
									i 
										display block
								i
									font-size 8pt
									padding-top 7.5px
									display none
									float left				
